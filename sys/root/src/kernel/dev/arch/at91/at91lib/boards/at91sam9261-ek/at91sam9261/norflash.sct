; *------------------------------------------------------------------------------
; *      Linker scatter for running in norflash on the AT91SAM9261
; *----------------------------------------------------------------------------*/

Load_region 0x10000000 0x10000000 {

    Fixed_region 0x10000000 {
       *(cstartup +First)
       .ANY (+RO)
    }
   
    Relocate_region 0x300000 0x28000 {
        *.o (VECTOR, +First)   
        board_lowlevel.o (+RO)
        board_memories.o (+RO)
        .ANY (+RW +ZI)
    }
    
    ARM_LIB_HEAP 0x326000 EMPTY 0x1000 {
    }
    
    ARM_LIB_STACK 0x328000 EMPTY -0x1000 {
    }
    
}
